                        {%- endif -%}
                        <div class="localization-wrapper --align-middle">
                          {%- liquid
                            assign lfid = section.id | prepend: 'language-form-mob'
                            assign if_id_no_script = section.id | prepend: 'language-form-mob' | append: '-noscript'
                      
                            assign resid = section.id | prepend: 'country-form-mob'
                            assign res_id_no_script = section.id | prepend: 'country-form-mob' | append: '-noscript'
