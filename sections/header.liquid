                                    <span class="button_icn-wrapper">
                                      {%- liquid
                                        render 'icons-data', width: 18, height: 18, use_icon: true, icon_reference: 'cart', section_id: section.id
                                        render 'cart-bubble'
                                        render 'cart-text'
                                      -%}
                                    </span>
                                    <span class="cart_btn-text">{{ 'cart.title' | t }}</span>
                                  </a>
                              {%- endunless -%}
                          {%- endif -%}
                        </li>
                      {%- endif -%}
                    </ul>
                  </div>
                {%- endif -%}
                {%- if local_exists == 'true' -%}
                  <div class="{{ section_prefix_class }}_mega-menu-mobile links-actions--items">
                    {%- unless action_exists == 'true' or cart_exists == 'true' -%}
                      {%- if announcement_exists == 'true' -%}
                        <div class="left-border"></div>
                      {%- endif -%}
                    {%- endunless -%}
                    <div class="localization-wrapper --align-middle">
                      {%- liquid
                        assign lfid = section.id | prepend: 'language-form-'
                        assign if_id_no_script = section.id | prepend: 'language-form-' | append: '-noscript'
                  
                        assign resid = section.id | prepend: 'country-form-'
                        assign res_id_no_script = section.id | prepend: 'country-form-' | append: '-noscript'
