{%- liquid
  assign section_prefix_class = 'page-header'
  assign section_scheme_class = section.settings.section_color_scheme | prepend: 'scheme scheme--'
  assign cart_view = settings.cart_view

  capture check_localization
    render 'check-localization-drops'
  endcapture

  capture local_exists
    if section.settings.enable_country_selector or section.settings.enable_language_selector
      if check_localization == 'true'
        echo 'true'
      endif
    endif
  endcapture

  capture enable_account_dashboard
    if shop.customer_accounts_enabled
      if customer
        echo 'true'
      endif
    endif
  endcapture

  capture action_exists
    if section.settings.show_customer or section.settings.enable_search
      echo 'true'
    endif
  endcapture

  capture cart_exists
    if section.settings.show_cart
      echo 'true'
    endif
  endcapture

  capture sticky_behavior
    case section.settings.enable_sticky
      when 'on_scroll_up'
        echo 'upwards'
      when 'on_scroll'
        echo 'downwards'
      else
        echo 'stacked'
    endcase
  endcapture

  capture mega_menu_exists
    if section.blocks.size > 0
      for block in section.blocks
        if block.type == 'mega_menu'
          echo 'true'
          break
        endif
      endfor
      else
        echo 'false'
    endif
  endcapture

  capture announcement_exists
    if section.blocks.size > 0
      for block in section.blocks
        if block.type == 'announcement_text'
          echo 'true'
          break
        endif
      endfor
      else
        echo 'false'
    endif
  endcapture

  capture predictive_tag
    if section.settings.enable_predictive_suggestions
      echo 'predictive-dropdown'
      else
        echo 'dropdown-component'
    endif
  endcapture

  capture media_height
    case section.settings.media_height
    when 'adapt'
        echo '--media-size-adapt'
      when 'small'
        echo '--small-height'
      when 'medium'
        echo '--medium-height'
    endcase
  endcapture

  capture section_container_layout
    case section.settings.section_container_layout
      when 'page-width'
        echo 'container'
      when 'contained-content'
        echo 'contained-container'
      when 'container-fluid'
        echo 'container-fluid'
    endcase
  endcapture

  capture section_corner_radius
    case section.settings.section_corner_radius
      when 'top_corners_radius'
        echo 'top_corners_radius'
      when 'bottom_corners_radius'
        echo 'bottom_corners_radius'
      when 'both_corners_radius'
        echo 'both_corners_radius'
      when 'no_corners_radius'
        echo 'no_corner_radius'
    endcase
  endcapture

  capture predictive_corner_radius
    case section.settings.predictive_corner_radius
    when 'top_corners_radius'
      echo 'top_corners_radius'
    when 'bottom_corners_radius'
      echo 'bottom_corners_radius'
    when 'both_corners_radius'
      echo 'both_corners_radius'
    when 'no_corners_radius'
      echo 'no_corner_radius'
  endcase
  endcapture

  assign marquee_direction = 'backward'
  if section.settings.marquee_direction == 'marquee_forward'
    assign marquee_direction = 'forward'
  endif

  assign multiply_factor = 6
  if section.blocks.size > 3
    assign multiply_factor = 4
    elsif section.blocks.size > 6
      assign multiply_factor = 2
  endif
-%}
