{%- liquid
  assign section_prefix_class = 'main-product'
  assign section_scheme_class = section.settings.section_color_scheme | prepend: 'scheme scheme--'
  assign first_3d_model = product.media | where: 'media_type', 'model' | first

  capture section_container_layout
    case section.settings.section_container_layout
      when 'page-width'
        echo 'container'
      when 'contained-content'
        echo 'contained-container'
      when 'container-fluid'
        echo 'container-fluid'
    endcase
  endcapture

  capture section_corner_radius
    case section.settings.section_corner_radius
      when 'top_corners_radius'
        echo 'top_corners_radius'
      when 'bottom_corners_radius'
        echo 'bottom_corners_radius'
      when 'both_corners_radius'
        echo 'both_corners_radius'
      when 'no_corners_radius'
        echo 'no_corner_radius'
    endcase
  endcapture

  capture product_clean_url
    if product.url contains '?'
      assign parts = product.url | split: '?'
      echo parts[0]
      else
        echo product.url
    endif
  endcapture

  assign product_title = product.title
  assign selected_variant = product.selected_or_first_available_variant
  assign current_values = selected_variant.options
  assign product_json_id = 'ProductCardJson-' | append: product.id | append: '-' | append: section.id
  assign form_id = 'main-product-atc-form' | append: product.id | append: '-' | append: section.id
  assign product_form_installment_id = 'main-product-atc-form-installment' | append: product.id | append: '-' | append: section.id
  assign error_render_attr = 'data-render-main-error' | append: product.id | append: '-' | append: section.id
  assign media_slider_attr = 'data-change-slide-' | append: product.id | append: '-' | append: section.id
  assign price_render_attr = 'data-render-price'
  assign price_unit_render_attr = 'data-render-price-unit'
  assign sku_render_attr = 'data-render-sku'
  assign stock_render_attr = 'data-render-stock'
  assign qty_render_attr = 'data-render-qty'
  assign pickup_render_attr = 'data-render-pickup'
  assign share_render_attr = 'data-render-share'
-%}
