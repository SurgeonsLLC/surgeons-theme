<div
  class="global-block {{ section_prefix_class }} {{ section_prefix_class }}-{{ section.id }} {% if section_container_layout == 'container-fluid' or section_container_layout == 'contained-container' %}{{ section_scheme_class }} {{ section_corner_radius }}{% endif %}"
  id="{{- section.id -}}"
  style="--section-padding-top: {{ section.settings.section_top_padding }}px; --section-padding-bottom: {{ section.settings.section_bottom_padding }}px;"
>
  {%- liquid
    assign container_index = -1
    assign full_width_index = -1
    assign has_left_blocks = false
    for block in section.blocks
      if container_index == -1 and block.type == 'column_separator'
        assign container_index = forloop.index0
      endif
    
      if full_width_index == -1 and block.type == 'full_width_separator'
        assign full_width_index = forloop.index0
      endif
    endfor
  -%}
  <div class="{{ section_container_layout }}">
    <div class="{{ section_prefix_class }}_wrapper {% if section_container_layout == 'container' %}{{ section_scheme_class }} {{ section_corner_radius }} internal--spacing{% endif %}" id="{{- section.id -}}-description">
      <div class="{{ section_prefix_class }}_wrapper-cols">
        {%- if product.media.size > 0 -%}
          <div class="{{ section_prefix_class }}_col {{ section_prefix_class }}_col--media">
            <div class="{{ section_prefix_class }}_sticky-wrap">
              <div class="{{ section_prefix_class }}_slider-wrap {% if product.media.size == 1 %}{{ section_prefix_class }}_slider-wrap--full{% endif %}">
                {%- render 'product-gallery-new', product: product, variant_media_attr: media_slider_attr, fetchpriority: true, section_prefix_class: section_prefix_class, zoom: true, media_size: section.settings.product_media_size, gallery_auto_play: section.settings.gallery_auto_play, gallery_show_navigation: section.settings.gallery_show_navigation, gallery_change_slides_every: section.settings.gallery_change_slides_every -%}
              </div>
              {%- liquid
                for block in section.blocks
                  if full_width_index != -1 and forloop.index0 >= full_width_index
                    continue
                  endif
                  if container_index != -1 and forloop.index0 > container_index
                    render 'product-blocks', block: block, product_title: product_title, product_clean_url: product_clean_url, error_render_attr: error_render_attr, share_render_attr: share_render_attr, pickup_render_attr: pickup_render_attr, qty_render_attr: qty_render_attr, sku_render_attr: sku_render_attr, stock_render_attr: stock_render_attr, price_render_attr: price_render_attr, product_form_installment_id: product_form_installment_id, form_id: form_id, current_values: current_values, product_json_id: product_json_id, price_unit_render_attr: price_unit_render_attr, media_slider_attr: media_slider_attr, section_prefix_class: section_prefix_class, extra_class: '--in-left-column'
                  endif
                endfor
              -%}
            </div>
          </div>
        {%- endif -%}
        <div class="{{ section_prefix_class }}_col {{ section_prefix_class }}_col--content {% if product.media.size == 0  %}--full_width{% endif %}">
          <div class="{{ section_prefix_class }}_sticky-wrap">
            {%- liquid
              for block in section.blocks
                if full_width_index != -1 and forloop.index0 >= full_width_index
                  continue
                endif
                if container_index == -1 or forloop.index0 < container_index
                  render 'product-blocks', block: block, product_title: product_title, product_clean_url: product_clean_url, error_render_attr: error_render_attr, share_render_attr: share_render_attr, pickup_render_attr: pickup_render_attr, qty_render_attr: qty_render_attr, sku_render_attr: sku_render_attr, stock_render_attr: stock_render_attr, price_render_attr: price_render_attr, product_form_installment_id: product_form_installment_id, form_id: form_id, current_values: current_values, product_json_id: product_json_id, price_unit_render_attr: price_unit_render_attr, media_slider_attr: media_slider_attr, section_prefix_class: section_prefix_class, extra_class: '--in-right-column'
                endif
              endfor
            -%}
          </div>
        </div>
      </div>
      {%- if full_width_index != -1 -%}
        <div class="{{ section_prefix_class }}_col {{ section_prefix_class }}_col--full-width">
          {%- liquid
            for block in section.blocks
              if full_width_index != -1 and forloop.index0 > full_width_index
                render 'product-blocks', block: block, product_title: product_title, product_clean_url: product_clean_url, error_render_attr: error_render_attr, share_render_attr: share_render_attr, pickup_render_attr: pickup_render_attr, qty_render_attr: qty_render_attr, sku_render_attr: sku_render_attr, stock_render_attr: stock_render_attr, price_render_attr: price_render_attr, product_form_installment_id: product_form_installment_id, form_id: form_id, current_values: current_values, product_json_id: product_json_id, price_unit_render_attr: price_unit_render_attr, media_slider_attr: media_slider_attr, section_prefix_class: section_prefix_class, extra_class: '--in-full-width'
              endif
            endfor
          -%}
        </div>
      {%- endif -%}
    </div>
  </div>
  <script id="{{- product_json_id -}}" type="application/json">
    {
      "handle": "{{ product.handle | escape }}",
      "available": {{ product.available | json }},
      "variants": [
        {%- for variant in product.variants -%}
          {
            "id": {{ variant.id | json }},
            "featured_image": {% unless variant.featured_media.id == blank %}{{ variant.featured_media.id }}{% else %}null{% endunless %},
            "available": {{ variant.available | json }},
            "options": {{ variant.options | json }},
            "option1": {{ variant.option1 | json }},
            "option2": {{ variant.option2 | json }},
            "option3": {{ variant.option3 | json }},
            "quantity_rule": {{ variant.quantity_rule | json }}
          }
          {% if forloop.last == false %},{% endif -%}
        {%- endfor -%}
      ]
    }
  </script>
  <script type="application/ld+json">
    {{ product | structured_data }}
  </script>
  {%- if first_3d_model -%}
    <script type="application/json" id="ProductJSON-{{ product.id }}">
      {{- product.media | where: 'media_type', 'model' | json -}}
    </script>
  {%- endif -%}
  {%- if request.design_mode == false -%}
    {%- if product.media.size > 0 -%}
      <link
        rel="stylesheet"
        href="{{ 'photoswipe.css' | asset_url }}"
        media="print"
        onload="this.media='all'"
      >
      <script type="module">
        import PhotoSwipeLightbox from '{{- 'photoswipe-lightbox.esm.js' | asset_url -}}';
        let options = {
          gallery: '#gallery-zoom-in-{{- section.id -}}{{- product.id -}}',
          children: '.--media-zoom-in',
          pswpModule: () => import('{{- 'photoswipe.esm.js' | asset_url -}}')
        };
        const lightbox = new PhotoSwipeLightbox(options);
        lightbox.init();
      </script>
    {%- endif -%}
  {%- endif -%}
</div>
