{%- liquid
  assign section_prefix_class = 'main-grid'
  assign section_scheme_class = section.settings.section_color_scheme | prepend: 'scheme scheme--'
  assign banner_color_scheme = section.settings.banner_color_scheme | prepend: 'scheme scheme--'
  assign sort_by = collection.sort_by | default: collection.default_sort_by
  assign results_url = collection.url
  assign base_url = request.path

  if section.settings.enable_sort
    capture sort_value
      for option in collection.sort_options
        if option.value == sort_by
          echo option.value
        endif
      endfor
    endcapture
    assign base_url = base_url | append: '?' | append: 'sort_by' | append: '=' | append: sort_by
  endif

  capture max_products_to_show
    assign columns_count = section.settings.number_of_columns_on_desktop | times: 1
    echo columns_count | times: section.settings.number_of_rows_on_desktop
  endcapture

  capture enable_description
    unless collection.description == blank
      if section.settings.show_description
        echo 'true'
      endif
    endunless
  endcapture

  capture section_container_layout
    case section.settings.section_container_layout
      when 'page-width'
        echo 'container'
      when 'contained-content'
        echo 'contained-container'
      when 'container-fluid'
        echo 'container-fluid'
    endcase
  endcapture

  capture section_corner_radius
    case section.settings.section_corner_radius
      when 'top_corners_radius'
        echo 'top_corners_radius'
      when 'bottom_corners_radius'
        echo 'bottom_corners_radius'
      when 'both_corners_radius'
        echo 'both_corners_radius'
      when 'no_corners_radius'
        echo 'no_corner_radius'
    endcase
  endcapture

  capture main_page_max_content_size
    echo settings.page_width | times: section.settings.max_content_size | divided_by: 100
  endcapture

  capture main_page_section_parallax_class
    if section.settings.parallax_effect
      echo '--parallax-effect'
    endif
  endcapture

  capture main_page_section_container_layout
    case section.settings.main_page_section_container_layout
      when 'page-width'
        echo 'container'
      when 'contained-content'
        echo 'contained-container'
      when 'container-fluid'
        echo 'container-fluid'
    endcase
  endcapture

  capture main_page_section_corner_radius
    case section.settings.main_page_section_corner_radius
      when 'top_corners_radius'
        echo 'top_corners_radius'
      when 'bottom_corners_radius'
        echo 'bottom_corners_radius'
      when 'both_corners_radius'
        echo 'both_corners_radius'
      when 'no_corners_radius'
        echo 'no_corner_radius'
    endcase
  endcapture

  capture main_page_section_height_class
    case section.settings.section_height
      when 'small'
        echo '--small-height'
      when 'medium'
        echo '--medium-height'
      when 'window_height'
        echo '--vh-height'
    endcase
  endcapture

  capture main_page_content_alignment
    case section.settings.content_alignment
      when 'top'
        echo 'main-page_tcenter'
      when 'middle'
        echo 'main-page_mcenter'
      when 'bottom'
        echo 'main-page_bcenter'
    endcase
  endcapture
  
-%}
{%- style -%}
  .main-page_bg-wrapper,
  .main-page.--parallax-effect .main-page_image,
  .main-page_bg-wrapper::before{
    position: absolute;
  }

  .breadcrumb-nav,
  .breadcrumb-list{
    background: var(--section-background-color);
  }

  .breadcrumb-nav,
  .breadcrumb-list,
  .breadcrumb-list_item + .breadcrumb-list_item::before {
    color: var(--section-heading-color);
  }

  .breadcrumb-nav {
    --bdbottom-width: 1px;
    --bdbottom-clr: var(--section-border-color);
  }

  .breadcrumb_wrapper,
  .main-page_bg-wrapper,
  .main-page_image,
  .main-page_wrapper,
  .main-page_bg-wrapper::before,
  .pagination-wrapper,
  .pagination,
  .pagination_list,
  .modal-component--facets-drawer .modal-component_wrapper{
    width: 100%;
  }

  .breadcrumb-nav,
  .main-page{
    padding-top: 0;
    padding-bottom: 0;
  }

  .breadcrumb-list,
  .breadcrumb-list_link{
    font-size: 1.2rem;
  }

  .breadcrumb-list{
    line-height: 2rem;
    font-family: var(--font-headings-family);
    padding: 12px 0;
  }

  .internal--spacing.breadcrumb-list,
  .main-page_content .heading-head{
    padding: 12px 22px;
  }

  .breadcrumb-list_item + .breadcrumb-list_item::before {
    content: '>';
    display: inline-block;
    margin-left: 11px;
    margin-right: 11px;
  }

  .breadcrumb-list [aria-current="page"],
  .breadcrumb-list_link{
    font-weight: 600;
  }


  .breadcrumb-list [aria-current="page"]{
    color: var(--heading-color);
  }

  .breadcrumb-list_link{
    text-decoration: none;
    transition: color 0.3s ease;
    --link-color: 
    color-mix(in srgb, var(--section-heading-color) 80%, var(--section-links-color) 20%);
  }

  .main-page_wrapper,
  .main-page_content,
  .main-page,
  .main-page_bg-wrapper::before,
  .grid-common_facets-bar{
    z-index: 1;
  }

  .main-page_wrapper{
    margin: auto;
    min-height: inherit;
  }

  .main-page_wrapper.--padding{
    --section-padding-top: 0;
    --section-padding-bottom: 0;
  }

  .main-page_content{
    flex-direction: column;
  }

  .main-page_tcenter,
  .main-page_mcenter{
    margin-bottom: auto;
  }

  .main-page_bcenter,
  .main-page_mcenter{
    margin-top: auto;
  }

  .main-page{
    --height-factor: 0.817;
    text-align: var(--mobile-text-alignment);
  }

  .main-page,
  .main-page_bg-wrapper{
    overflow: hidden;
  }

  .main-page_bg-wrapper,
  .main-page_image,
  .main-page_wrapper,
  .main-page_bg-wrapper::before{
    height: 100%;
  }

  .main-page_bg-wrapper,
  .main-page.--parallax-effect .main-page_image,
  .main-page_bg-wrapper::before{
    top: 0;
    left: 0;
  }

  .main-page_bg-wrapper,
  .main-page_image{
    z-index: -1;
  }

  .main-page_bg-wrapper svg,
  .main-page_bg-wrapper img,
  .main-page_bg-wrapper,
  .main-page_bg-wrapper::before,
  .main-page_image{
    border-radius: inherit;
  }

  .main-page.--parallax-effect .main-page_image {
    background-attachment: fixed;
    background-position: 50% 50%;
    background-size: cover;
  }

  .main-page_wrapper{
    --sec-pt-top: calc(var(--section-padding-top) * 0.45);
    --sec-pt-bottom: calc(var(--section-padding-bottom) * 0.45);
    padding-top: var(--sec-pt-top, 35px);
    padding-bottom: var(--sec-pt-bottom, 35px);
  }

  .main-page_bg-wrapper::before{
    content: "";
    background: var(--overlay-bg);
  }

  .main-page_description {
    --mt-top: 17px;
    color: color-mix(in srgb, var(--section-heading-color) 80%, var(--section-foreground-color) 20%);
  }

  .main-page_content .inner-container{
    max-width: var(--max-content);
  }

  .pagination-wrapper{
    padding-left: var(--cp-spacer-left, 15px);
    padding-right: var(--cp-spacer-right, 15px);
  }

  .pagination_list {
    gap: 6px;
  }

  .pagination_next-pg{
    margin-right: 9.6px;
  }

  .pagination_back-pg{
    margin-left: 9.6px;
  }

  .pagination_item{
    border: 1px solid var(--section-border-color);
    border-radius: var(--section-radius);
  }

  .pagination_link{
    text-decoration: none;
    font-weight: 400;
    line-height: 2rem;
    transition: all 0.35s ease;
    padding: 0.5rem 1rem;
    border-radius: var(--section-radius);
  }

  a.pagination_link--active,
  .pagination_link:hover{
    background-color: var(--section-active-hover-color);
    color: var(--section-background-color);
  }

  .pagination-wrapper{
    margin-top: 47px;
  }

  .pagination-wrapper,
  .pagination,
  .pagination_list{
    justify-content: center;
  }

  .load-more_button{
    gap: 8px;
  }

  .load-more_loader{
    line-height: 1rem;
  }

  .modal-component--quick-view .deferred-media__poster-icn{
    width: 40px;
    height: 40px;
    top: 2px;
    left: auto;
    right: 2px;
    transform: unset;
    line-height: 0;
    border-radius: 0;
    background-color: var(--section-background-color, #fff);
    color: var(--section-heading-color, #fff);
    z-index: 1;
    opacity: 100%;
  }

  .modal-component--quick-view .deferred-media__poster-icn::before{
    content: "";
    position: absolute;
    top: 0;
    z-index: 999;
    left: 0;
    right: 0;
    bottom: 0;
    display: block;
    border: 1px solid var(--section-border-color);
    opacity: 0.5;
  }

  .modal-component--quick-view .deferred-media__poster-icn svg{
    opacity: 100%;
  }

  .modal-component--quick-view .deferred-media__poster:hover .deferred-media__poster-icn svg{
    opacity: 55%;
    transition: all 0.3s ease;
  }

  .grid-common_text{
    --mt-top: 20px;
  }

  .dropdown-sort_table,
  .dropdown-sort_wrap,
  .main-grid_featured-img,
  .main-page_wrapper-container,
  .main-page_content,
  .grid-common_facets-bar{
    position: relative;
  }

  .grid-common_empty-icn,
  .pagination_link {
    display: block;
  }

  .grid-common_opener,
  .grid-common_facets-bar,
  .grid-common_facet-opener,
  .facet-remove-btn,
  .grid-common_active-facets,
  .grid-common_facets-bar_col--sort,
  .modal-component--facets-drawer .modal-component_heading,
  .grid-common_sort-count,
  .load-more_button,
  .pagination-wrapper,
  .pagination,
  .pagination_list,
  .breadcrumb-list,
  .breadcrumb-list_item,
  .breadcrumb-nav,
  .main-page_wrapper,
  .main-page_content,
  .product-column_error,
  .modal-component--quick-view .deferred-media__poster-icn,
  .product-column_tag-wrap,
  .product-column,
  .product-column_btn-wrap,
  .product-column_btn,
  .product-column_tag-in,
  .product-column_top-wrap{
    display: flex;
  }

  .facet-remove-btn,
  .modal-component--facets-drawer .modal-component_heading{
    gap: 9px;
  }

  .grid-common_facet-opener,
  .grid-common_facets-bar{
    gap: 5px;
  }

  .grid-common_opener{
    height: 4.2rem;
    min-width: 168px;
  }

  .grid-common_sort-wrap,
  .dropdown-sort_table,
  .dropdown-sort_wrap,
  .grid-common_text-count,
  .grid-common_facets-sort {
    margin-left: auto;
  }

  .facet-sort_btn-submit,
  .grid-common_opener{
    line-height: 2.1rem;
    padding: 1.2rem;
  }

  .pagination_link,
  .facet-sort_btn-submit,
  .grid-common_opener{
    font-size: 1.4rem;
  }

  .facet-sort_btn-submit{
    background-color: transparent;
    color: inherit;
  }

  .facet-sort_btn-submit:hover, 
  .facet-sort_btn-submit.active {
    color: var(--section-input-background-color);
    background-color: var(--section-input-text-color);
  }

  .grid-common_opener,
  .dropdown-sort_wrapper{
    background-color: var(--section-input-background-color);
    color: var(--section-input-text-color);
    border: var(--input-border-width) solid var(--section-secondary-border-color);
    border-radius: var(--input-corner-radius);
  }

  .dropdown-sort_wrapper{
    border-top-left-radius: 0;
    border-top-right-radius: 0;
    border-width: 0 var(--input-border-width) var(--input-border-width);
    transform: none;
    left: 0;
    max-height: calc(40vh - 15px);
    overflow-y: auto;
    border-bottom-left-radius: var(--section-radius);
    border-bottom-right-radius: var(--section-radius);
  }

  .grid-common_facets-bar_col--sort,
  .dropdown-sort_title{
    flex-grow: 1;
  }

  .grid-common_facet-opener.button.button--link,
  .facet-remove-btn{
    --button-font-family: var(--font-headings-family);
  }

  .facet-remove-btn{
    --button-font-weight: 500;
  }

  .grid-common_facet-opener.button.button--link{
    --button-text: color-mix(in srgb, var(--section-heading-color) 80%, var(--section-links-color) 20%);
    --button-font-weight: 700;
    --button-padding-x: 0.5rem;
  }

  .grid-common_facet-opener .button_icn-wrapper,
  .modal-component--facets-drawer .modal-component_heading-icn{
    line-height: 0;
  }

  .grid-common_facet-opener{
    --button-font-size: 1.6rem;
    --button-line-height: 2.3rem;
    justify-content: flex-start;
    text-transform: uppercase;
  }

  .facet-remove-btn,
  .facets-remove-all.button--link{
    --button-padding-y: 0.4rem;
    --button-padding-x: 1rem;
  }

  .grid-common_facets-bar{
    margin-bottom: 16px;
  }

  .grid-common_active-facets{
    gap: 7px;

  }

  .breadcrumb-list,
  .grid-common_active-facets,
  .pagination_list {
    flex-wrap: wrap;
  }

  .grid-common_sort-count,
  .modal-component--facets-drawer .modal-component_heading,
  .load-more_button{
    align-items: center;
  }

  .modal-component--facets-drawer .modal-component_heading {
    --mt-bottom: 16px;
    text-transform: uppercase;
  }

  .modal-component--facets-drawer .modal-component_wrapper{
    max-width: calc(var(--cp-width, 1200) * 1px);
    height: 276px;
    color: var(--section-foreground-color);
    box-shadow: 0 20px 20px #00000026;
    margin-right: auto;
    padding: 20px 40px;
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    border-top-left-radius: var(--section-radius);
    border-top-right-radius: var(--section-radius);
  }

  .modal-component--facets-drawer .modal-component_content{
    border-top: 1px solid var(--section-border-color);
  }

  .modal-component--facets-drawer.--drawer{
    bottom: 0;
  }

  .modal-component--facets-drawer .modal-component_btn-close{
    top: 24px;
    right: 33px;
  }

  .main-grid{
    --desktop-columns-count: 1;
  }

  .main-grid_featured-img{
    width: 100px;
    height: 100px;
    margin: 0 auto 14px;
    border: 1px solid var(--section-border-color);
  }

  .main-grid_featured-img,
  .main-grid_featured-img img{
    border-radius: var(--section-radius);
  }

  .active-facets--desktop{
    display: none;
  }

  .active-facets--mobile{
    margin: 16px 0;
  }

  .grid-common_sort-count{
    gap: 10px;
    flex-direction: column;
  }

  .modal-component--facets-drawer{
    padding: 0 22px;
  }

  .main-grid_cols-wrap{
    --cp-spacer-left: 12px;
    --cp-spacer-right: 12px;
  }

  .product-column_image,
  .product-column_tag,
  .product-column .tags-list_tag,
  .product-column .product-tag_dot,
  .product-column_vendor{
    display: block;
  }

  .product-column_image:focus-visible{
    --focus-outline-offset: 0;
  }

  .dropdown-sort_opener{
    --focus-outline-offset: 1px;
  }

  .product-column_image:focus-visible > img,
  .product-column_image:focus-visible > svg{
    padding: 4px;
  }

  .product-column,
  .product-column_image:focus-visible > img{
    border: 1px solid var(--section-border-color);
  }

  .product-column,
  .product-column_top-wrap{
    flex-direction: column;
  }

  .product-column{
    border-radius: var(--section-radius);
  }

  .product-column_image,
  .product-column_image > img,
  .product-column_image > svg{
    border-radius: var(--section-radius) var(--section-radius) 0 0;
  }

  .product-column,
  .product-column_image,
  .product-column_btn-wrap{
    width: 100%;
  }

  .product-column_image,
  .product-column_image-wrap,
  .product-column_media-wrap{
    position: relative;
  }

  .product-column_tag-wrap{
    position: absolute;
    left: 17px;
    top: 14px;
    right: 17px;
  }

  .product-column_btn{
    --button-outline-offset: -2px;
    --button-font-weight: 600;
    --button-font-family: var(--font-headings-family);
    --button-padding-x: 1.9rem;
  }
  
  .product-column_tag-wrap{
    flex-wrap: wrap;
    gap: 5px;
  }

  .product-column_btn .product-column_btn-txt{
    padding-left: 11px;
  }

  .product-column_tag-in,
  .product-column_tag,
  .product-column_tag-dot,
  .product-column_price .product-price_unit-pricing::before{
    border-radius: calc(var(--section-radius) * 99);
  }

  .product-column_tag{
    padding: 6px 9px;
    background-color: var(--badge-bg);
    color: var(--badge-clr);
  }

  .product-column_tag-dot{
    width: 6px;
    height: 6px;
    background-color: currentColor;
  }

  .product-column_tag-in,
  .product-column_tag{
    line-height: 1rem;
  }

  .product-column_tag-in{
    padding: 5px 9px;
    column-gap: 6px;
    background-color: var(--bg-clr);
  }

  .product-column_content{
    flex-grow: 1;
    padding: 20px 17px 17px 17px;
  }

  {% if section.settings.enable_quick_view and section.settings.enable_atc %}
    #{{ section.id }} .product-column_quick-wrap{
      padding-right: 8px;
      background-size: 8px 8px;
      background-repeat: no-repeat;
      background-position: 100% calc(50% - 2px), 100% calc(50% + 4px);
    }

    #{{ section.id }} .product-column_quick-wrap--sep{
      background-image: radial-gradient(circle 4px at top center, transparent 4px, var(--section-primary-border-color) 4px), radial-gradient(circle 4px at bottom center, transparent 4px, var(--section-primary-border-color) 4px);
    }

    #{{ section.id }} .product-column_btn .product-column_btn-txt{
      padding-left: 0;
    }

    #{{ section.id }} .product-column_btn,
    #{{ section.id }} .product-column_quick-wrap:has(.product-column_btn--quick:hover) + .product-column_atc-wrap .product-column_btn--atc,
    #{{ section.id }} .product-column_quick-wrap:has(.product-column_btn--quick:focus-visible) + .product-column_atc-wrap .product-column_btn--atc,
    #{{ section.id }} .product-column_btn--atc{
      --button-padding-x: 1rem;
    }
    #{{ section.id }} .product-column_btn-txt{
      display: none;
    }
  {% endif %}

  .product-column_vendor{
    margin-bottom: 7px;
  }

  .product-column_title a{
    text-decoration: none;
  }

  .product-column_price .main-price,
  .product-column_price .regular-price{
    font-size: 1.4rem;
    line-height: 2.2rem;
  }

  .product-column_price .main-price{
    font-weight: 500;
  }

  .product-column_price .main-price,
  .product-column .product-column_error>.ico{
    color: var(--section-error-color);
  }

  .product-column_error{
    background: var(--section-error-color);
    margin: 6px 0;
    padding: 6px;
    font-family: var(--font-headings-family);
  }

  .product-column_error,
  .product-column_price .product-price_unit-pricing{
    font-size: 1.2rem;
  }

  .product-column_tag-in,
  .product-column_error{
    color: var(--section-background-color);
  }

  .breadcrumb-list_item + .breadcrumb-list_item::before,
  .product-column .product-column_error>.ico{
    flex-shrink: 0;
  }

  .product-column_price .product-price_unit-pricing{
    line-height: 1.8rem;
    text-transform: capitalize;
    color: var(--section-foreground-color);
  }

  .product-column_price .product-price_unit-pricing::before{
    content: '';
    display: inline-block;
    width: 4px;
    height: 4px;
    margin-bottom: 3px;
    margin-left: 4px;
    margin-right: 2px;
    background: var(--section-foreground-color);
  }

  .product-column_price-wrap{
    margin-top: 8px;
  }

  .product-column_btn-wrap{
    margin-top: 14px;
  }

  .product-column_media-wrap,
  .product-column,
  .product-column_top-wrap{
    height: 100%;
  }

  .main-grid_wrapper.internal--spacing{
    padding: 10px 15px;
  }

  @media (min-width: 576px){
    .main-grid{
      --desktop-columns-count: 2;
    }
    .pagination_link {
      padding: 1rem 1.6rem;
    }
  }

  @media (min-width: 768px){
    .grid-common_facets-bar_col--filter{
      display: flex;
      align-items: center;
      gap: 4px;
    }

    {% if section.settings.enable_quick_view and section.settings.enable_atc %}
      #{{ section.id }} .product-column_btn--atc{
        --button-padding-x: 1.9rem;
      }
      #{{ section.id }} .product-column_btn-txt{
        display: block;
      }
      #{{ section.id }} .product-column_btn,
      #{{ section.id }} .product-column_btn-txt{
        transition: all 0.3s ease;
      }
      
      #{{ section.id }} .product-column_btn .product-column_btn-txt{
        padding-left: 0;
      }

      #{{ section.id }} .product-column_btn-txt,
      #{{ section.id }} .product-column_quick-wrap:has(.product-column_btn--quick:hover) + .product-column_atc-wrap .product-column_btn--atc .product-column_btn-txt,
      #{{ section.id }} .product-column_quick-wrap:has(.product-column_btn--quick:focus-visible) + .product-column_atc-wrap .product-column_btn--atc .product-column_btn-txt{
        max-width: 0;
      }

      #{{ section.id }} .product-column_btn-txt,
      #{{ section.id }} .product-column_btn-wrap{
        overflow: hidden;
      }

      #{{ section.id }} .product-column_btn-txt{
        white-space: nowrap;
      }

      #{{ section.id }} .product-column_btn:hover,
      #{{ section.id }} .product-column_btn:focus-visible{
        --button-padding-x: 1.9rem;
      }

      #{{ section.id }} .product-column_btn:hover .product-column_btn-txt,
