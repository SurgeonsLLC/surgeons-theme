<div class="global-block main-page main-page-{{ section.id }} {{ main_page_section_parallax_class }} {% if main_page_section_container_layout == 'container-fluid' or main_page_section_container_layout == 'contained-container' %}{{ banner_color_scheme }} {{ main_page_section_corner_radius }}{% endif %}" style="--desktop-text-alignment: {{ section.settings.section_text_alignment }}; --mobile-text-alignment: {{ section.settings.section_mobile_text_alignment }};">
  <div class="main-page_wrapper --padding">
    <div class="{{ main_page_section_container_layout }} {% if main_page_section_container_layout == 'container' %}main-page_wrapper-container{% endif %}">
      <div class="main-page_content {{ main_page_section_height_class }} {% if main_page_section_container_layout == 'container' %}{{ banner_color_scheme }} {{ main_page_section_corner_radius }} internal--spacing{% endif %}">
        <header class="heading-head inner-container --word-break --q-align {{ main_page_content_alignment }}" style="--max-content: {{ main_page_max_content_size }}px">
          {%- if section.settings.show_featured_image -%}
            {%- unless collection.featured_image == blank -%}
              {%- liquid
                capture image_alt
                  if collection.featured_image.alt != blank
                    echo collection.featured_image.alt
                    else
                      echo collection.title | escape
                  endif
                endcapture
              -%}
              {%- capture image_thumb_sizes -%}
                (max-width: 767px) 100px, 200px
              {%- endcapture -%}
              <div
                class="{{ section_prefix_class }}_featured-img {% if settings.enable_viewport_anim %}viewport-block{% endif %}"
                data-margin="0px 0px -5px 0px"
                data-frames='[{"opacity":"0","transform":"translateY(15px)"},{"opacity":"1","transform":"translateY(0px)"}]'
                data-options='{"duration":1000,"delay":0,"easing":"ease","fill":"both","iterations":1}'
              >
                <img
                  srcset="{%- if collection.featured_image.width >= 100 -%}{{ collection.featured_image | image_url: width: 100 }} 100w,{%- endif -%}
                          {%- if collection.featured_image.width >= 200 -%}{{ collection.featured_image | image_url: width: 200 }} 200w{%- endif -%}"
                  sizes="{{- image_thumb_sizes -}}"
                  src="{{- collection.featured_image | image_url: width: 200 -}}"
                  alt="{{- image_alt -}}"
                  fetchpriority="high"
                  width="200"
                  height="{{- 200 | divided_by: collection.featured_image.aspect_ratio | round -}}"
                  class="object-fit object-fit--absolute"
                >
              </div>
            {%- endunless -%}
          {%- endif -%}
          {%- if section.settings.section_caption != blank -%}
            <div
              class="main-page_caption h --word-break {{ section.settings.section_caption_size }} {{ section.settings.section_caption_weight }} mty {% if settings.enable_viewport_anim %}viewport-block{% endif %}"
              data-margin="0px 0px -5px 0px"
              data-frames='[{"opacity":"0","transform":"translateY(15px)"},{"opacity":"1","transform":"translateY(0px)"}]'
              data-options='{"duration":1000,"delay":250,"easing":"ease","fill":"both","iterations":1}'
            >
              {{- section.settings.section_caption -}}
            </div>
          {%- endif -%}
          {%- if collection.title != blank -%}
            <h1
              class="main-page_heading {{ section.settings.section_heading_size }} {{ section.settings.section_heading_weight }} mty {% if settings.enable_viewport_anim %}viewport-block{% endif %}"
              data-margin="0px 0px -5px 0px"
              data-frames='[{"opacity":"0","transform":"translateY(15px)"},{"opacity":"1","transform":"translateY(0px)"}]'
              data-options='{"duration":1000,"delay":350,"easing":"ease","fill":"both","iterations":1}'
            >
              {{- collection.title -}}
            </h1>
          {%- endif -%}
          {%- if enable_description == 'true' and section.settings.show_description == 'top' -%}
            <div
              class="main-page_description --h-anchor-none {{ section.settings.description_text_size }} {{ section.settings.section_description_weight }} mty {% if settings.enable_viewport_anim %}viewport-block{% endif %}"
              data-margin="0px 0px -5px 0px"
              data-frames='[{"opacity":"0","transform":"translateY(15px)"},{"opacity":"1","transform":"translateY(0px)"}]'
              data-options='{"duration":1000,"delay":400,"easing":"ease","fill":"both","iterations":1}'
            >
              {{- collection.description -}}
            </div>
          {%- endif -%}
        </header>
        {%- if main_page_section_container_layout == 'container' -%}
          <div
            class="main-page_bg-wrapper"
            style="--overlay-bg: {{ section.settings.background }}"
