                                      data-forms='["#{{ product_form_installment_id }}", "#{{ form_id }}"]'
                                      data-error-selector="[{{ error_render_attr }}]"
                                      data-renders='["[{{ stock_render_attr }}]", "[{{ price_render_attr }}]", "[{{ price_unit_render_attr }}]", "[{{ sku_render_attr }}]", "[{{ qty_render_attr }}]", "[{{ pickup_render_attr }}]"]'
                                      data-section-id="{{- section.id -}}"
                                      data-product-url="{{- product_clean_url -}}"
